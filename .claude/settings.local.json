{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(npm install)",
      "Bash(npm install:*)",
      "Bash(mkdir:*)",
      "Bash(npm run dev:*)",
      "Bash(npm run build:*)",
      "Bash(git checkout:*)",
      "Bash(git fetch:*)",
      "Bash(ls:*)",
      "Bash(git stash:*)",
      "Bash(git push:*)",
      "Bash(npm run deploy:*)",
      "Bash(npm run lint)",
      "Bash(pkill:*)",
      "Bash(true)",
      "Bash(rm:*)",
      "Bash(timeout 10s npm run dev)",
      "WebFetch(domain:github.com)",
      "Bash(find:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)"
    ],
    "deny": []
  }
}